{
  "name": "sagiri",
  "version": "2.0.1",
  "description": "A simple, lightweight and actually good JS wrapper for the SauceNAO API.",
  "author": "sr229",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "runkitExampleFilename": "./dist/examples/example.js",
  "homepage": "https://github.com/ClarityCafe#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ClarityCafe/Sagiri"
  },
  "bugs": {
    "url": "https://github.com/ClarityCafe/issues"
  },
  "scripts": {
    "test": "jest --coverage",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "prebuild": "yarn lint && yarn test",
    "build": "tsc",
    "start": "yarn build -w",
    "prepublishOnly": "yarn build && yarn docs",
    "predocs": "yarn docs:clean && yarn lint && yarn docs:build",
    "docs": "jsdoc2md --template ./docs/template.hbs --files ./docsbuild/*.js --example-lang ts > ./README.md",
    "postdocs": "yarn docs:replace && yarn docs:clean",
    "docs:clean": "rimraf ./docsbuild",
    "docs:build": "tsc -p ./tsconfig.docs.json",
    "docs:replace": "replace --silent \".&lt;\" \"&lt;\" ./README.md"
  },
  "keywords": [
    "hitorigoto",
    "sagiri",
    "saucenao",
    "simple",
    "easy",
    "api"
  ],
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "dependencies": {
    "form-data": "^2.5.0",
    "node-fetch": "^2.6.0"
  },
  "devDependencies": {
    "@types/dotenv": "6.1.1",
    "@types/jest": "24.0.17",
    "@types/node": "12.7.2",
    "@types/node-fetch": "2.5.0",
    "@typescript-eslint/eslint-plugin": "1.13.0",
    "@typescript-eslint/parser": "1.13.0",
    "dotenv": "8.0.0",
    "eslint": "^6.1.0",
    "eslint-config-clarity": "0.2.3",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-prettier": "3.1.0",
    "eslint-plugin-react": "7.14.3",
    "eslint-plugin-react-hooks": "1.7.0",
    "eslint-plugin-sift": "0.3.0",
    "jest": "24.9.0",
    "jsdoc-to-markdown": "5.0.0",
    "prettier": "1.18.2",
    "replace": "1.1.0",
    "ts-jest": "24.0.2",
    "ts-node": "8.3.0",
    "typescript": "3.5.3"
  },
  "files": [
    "dist"
  ]
}
